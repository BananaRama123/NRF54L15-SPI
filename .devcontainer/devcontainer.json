{
  "name": "NRF54L15 SPI Development",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "nrf-spi-dev",
  "workspaceFolder": "/workspace/nrf-spi-test",
  "customizations": {
    "vscode": {
      "extensions": [
        "nordic-semiconductor.nrf-connect",
        "ms-vscode.cpptools",
        "marus25.cortex-debug",
        "ms-vscode.cmake-tools"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "cmake.configureOnOpen": false
      }
    }
  },
  "postAttachCommand": "echo 'Connected to NRF development container'",
  "remoteUser": "root"
}